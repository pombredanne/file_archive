#: file_archive/main.py:28
msgid ""
"Metadata should have format key=value, key=type:value (eg. age=int:23) or "
"key=type:req (eg. age=int:>21)\n"
msgstr "Les métadonnées doivent être dans le format clef=valeur, clef=type:valeur (ex age=int:23) ou clef=type:requête (ex age=int:>18)\n"

#: file_archive/main.py:46 file_archive/main.py:97
#, python-brace-format
msgid ""
"Metadata has unknown type '{t}'! Only 'str' and 'int' are supported.\n"
"If you meant a string with a ':', use 'str:mystring'"
msgstr "Une métadonnée a un type inconnu '{t}' ! Seuls \"str\" et \"int\" sont supportés.\nSi vous vouliez une chaîne contenant un \":\", utilisez \"str:chaine\"."

#: file_archive/main.py:58
#, python-brace-format
msgid "Differing types for conditions on key {k}: {t1}, {t2}\n"
msgstr "Types différents pour les conditions sur la clef {k} : {t1}, {t2}\n"

#: file_archive/main.py:63
#, python-brace-format
msgid "Multiple conditions {cond} on key {k}\n"
msgstr "Plusieurs conditions {cond} sur la clef {k}\n"

#: file_archive/main.py:80
msgid ""
"Metadata should have format key=value or key=type:value (eg. age=int:23)\n"
msgstr "Les métadonnées doivent être dans le format clef=valeur ou clef=type:valeur (ex age=int:23)\n"

#: file_archive/main.py:112
msgid "Missing filename\n"
msgstr "Nom de fichier manquant\n"

#: file_archive/main.py:116
#, python-brace-format
msgid "Path does not exist: {path}\n"
msgstr "Le nom de fichier n'existe pas : {path}\n"

#: file_archive/main.py:158 file_archive/main.py:221
#, python-brace-format
msgid "Unknown option: {opt}\n"
msgstr "Option inconnue : {opt}\n"

#: file_archive/main.py:229
msgid "Objectid not found\n"
msgstr "Objectid introuvable\n"

#: file_archive/main.py:236
msgid "No match found\n"
msgstr "Aucune correspondance\n"

#: file_archive/main.py:243
msgid "Warning: more matching files exist\n"
msgstr "Attention : il y a plusieurs fichiers correspondant\n"

#: file_archive/main.py:256
msgid "Error: match found but is a directory\n"
msgstr "Erreur : correspondance trouvée mais c'est un dossier\n"

#: file_archive/main.py:286
#, python-brace-format
msgid ""
"Error: not removing files unconditionally unless -f is given\n"
"(command would have removed {nb} files)\n"
msgstr "Erreur : pas de suppression inconditionnelle sauf si -f est passé\n(la commande aurait supprimé {nb} fichiers)\n"

#: file_archive/main.py:301
msgid "verify command accepts no argument\n"
msgstr "La commande verify n'accepte aucun paramètre\n"

#: file_archive/main.py:308
msgid "view command accepts no argument\n"
msgstr "La commande view n'accepte aucun paramètre\n"

#: file_archive/main.py:328
#, python-brace-format
msgid ""
"usage: {bin} <store> create\n"
"   or: {bin} <store> add <filename> [key1=value1] [...]\n"
"   or: {bin} <store> write [key1=value1] [...]\n"
"   or: {bin} <store> query [-d] [-t] [key1=value1] [...]\n"
"   or: {bin} <store> print [-m] [-t] <filehash> [...]\n"
"   or: {bin} <store> print [-m] [-t] [key1=value1] [...]\n"
"   or: {bin} <store> remove [-f] <filehash>\n"
"   or: {bin} <store> remove [-f] <key1=value1> [...]\n"
"   or: {bin} <store> verify\n"
"   or: {bin} <store> view\n"
msgstr "utilisation : {bin} <dépôt> create\n   ou: {bin} <dépôt> add <fichier> [clef1=valeur1] [...]\n   ou: {bin} <dépôt> write [clef1=valeur1] [...]\n   ou: {bin} <dépôt> query [-d] [-t] [clef1=valeur1] [...]\n   ou: {bin} <dépôt> print [-m] [-t] <hash> [...]\n   ou: {bin} <dépôt> print [-m] [-t] [clef1=valeur1] [...]\n   ou: {bin} <dépôt> remove [-f] <hash>\n   ou: {bin} <dépôt> remove [-f] <clef1=valeur1> [...]\n   ou: {bin} <dépôt> verify\n   ou: {bin} <dépôt> view\n"

